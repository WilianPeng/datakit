<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.action.extension" value="action" />
	<constant name="struts.serve.static.browserCache" value="false" />
	
	<package name="datakit" namespace="/account" extends="struts-default">
		<action name="login" method="login" class="com.datakit.account.action.LoginAction">
			<result name="success">/success.jsp</result>
			<result name="error">/login.jsp</result>
		</action>
		<action name="register" method="register" class="com.datakit.account.action.LoginAction">
			<result name="success">/success.jsp</result>
			<result name="error">/login.jsp</result>
		</action>
	</package>
	
</struts> 
